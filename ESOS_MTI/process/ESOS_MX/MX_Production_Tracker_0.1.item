    <elementParameter field="MEMO_SQL" name="QUERY" value="&quot;&#xD;&#xA;CREATE TABLE V_MX_WIP_vs_Orders AS&#xD;&#xA;WITH&#xD;&#xA;-- Krok 1: Pobranie zamwie&#xD;&#xA;Orders AS (&#xD;&#xA;    SELECT &#xD;&#xA;        mpo.`Document No_`,&#xD;&#xA;        mpo.`Expected Receipt Date`,&#xD;&#xA;        mpo.SKU,&#xD;&#xA;        mpo.Description,&#xD;&#xA;        mpo.order_by, &#xD;&#xA;        COUNT(mpo.SKU) AS Order_Count&#xD;&#xA;    FROM &#xD;&#xA;        MX_ProductionOrders mpo &#xD;&#xA;    WHERE &#xD;&#xA;        mpo.SKU IS NOT NULL&#xD;&#xA;    GROUP BY  &#xD;&#xA;        mpo.`Document No_`, &#xD;&#xA;        mpo.`Expected Receipt Date`, &#xD;&#xA;        mpo.SKU, &#xD;&#xA;        mpo.Description, &#xD;&#xA;        mpo.order_by &#xD;&#xA;),&#xD;&#xA;-- Krok 2: Zliczenie dostpnych zasobw WIP&#xD;&#xA;WIP AS (&#xD;&#xA;    SELECT &#xD;&#xA;        pl.SKU,&#xD;&#xA;        COUNT(pl.Barcode) AS Total_WIP_Count&#xD;&#xA;    FROM &#xD;&#xA;        MX_ProductionLogs pl&#xD;&#xA;    LEFT JOIN &#xD;&#xA;        MX_SKU_Definition msd  &#xD;&#xA;            ON TRIM(pl.SKU) = TRIM(msd.MMT_MTI_Code)&#xD;&#xA;    WHERE &#xD;&#xA;        pl.Area_Name NOT IN ('Packaging', 'Shipping')&#xD;&#xA;        AND pl.SKU IS NOT NULL&#xD;&#xA;    GROUP BY &#xD;&#xA;        pl.SKU&#xD;&#xA;),&#xD;&#xA;-- Krok 3: Zliczenie iloci w Packaging&#xD;&#xA;Packaging AS (&#xD;&#xA;    SELECT&#xD;&#xA;        pl.Sales_Order AS `Document No_`,&#xD;&#xA;        pl.SKU,&#xD;&#xA;        COUNT(pl.Barcode) AS Packaging_Count&#xD;&#xA;    FROM&#xD;&#xA;        MX_ProductionLogs pl&#xD;&#xA;    WHERE&#xD;&#xA;        pl.Area_Name = 'Packaging'&#xD;&#xA;        AND pl.SKU IS NOT NULL&#xD;&#xA;    GROUP BY&#xD;&#xA;        pl.Sales_Order,&#xD;&#xA;        pl.SKU&#xD;&#xA;),&#xD;&#xA;-- Krok 4: Zliczenie iloci w Shipping&#xD;&#xA;Shipping AS (&#xD;&#xA;    SELECT&#xD;&#xA;        pl.Sales_Order AS `Document No_`,&#xD;&#xA;        pl.SKU,&#xD;&#xA;        COUNT(pl.Barcode) AS Shipping_Count&#xD;&#xA;    FROM&#xD;&#xA;        MX_ProductionLogs pl&#xD;&#xA;    WHERE&#xD;&#xA;        pl.Area_Name = 'Shipping'&#xD;&#xA;        AND pl.SKU IS NOT NULL&#xD;&#xA;    GROUP BY&#xD;&#xA;        pl.Sales_Order,&#xD;&#xA;        pl.SKU&#xD;&#xA;),&#xD;&#xA;-- Krok 5: Poczenie danych&#xD;&#xA;Orders_With_Data AS (&#xD;&#xA;    SELECT&#xD;&#xA;        o.*,&#xD;&#xA;        COALESCE(w.Total_WIP_Count, 0) AS Total_WIP_Count,&#xD;&#xA;        COALESCE(p.Packaging_Count, 0) AS Packaging_Count,&#xD;&#xA;        COALESCE(s.Shipping_Count, 0) AS Shipping_Count&#xD;&#xA;    FROM&#xD;&#xA;        Orders o&#xD;&#xA;    LEFT JOIN&#xD;&#xA;        WIP w ON o.SKU = w.SKU&#xD;&#xA;    LEFT JOIN&#xD;&#xA;        Packaging p ON o.`Document No_` = p.`Document No_` AND o.SKU = p.SKU&#xD;&#xA;    LEFT JOIN&#xD;&#xA;        Shipping s ON o.`Document No_` = s.`Document No_` AND o.SKU = s.SKU&#xD;&#xA;),&#xD;&#xA;-- Krok 6: Obliczenie Adjusted_Order_Count&#xD;&#xA;Orders_Adjusted AS (&#xD;&#xA;    SELECT&#xD;&#xA;        owd.*,&#xD;&#xA;        GREATEST(owd.Order_Count - owd.Packaging_Count - owd.Shipping_Count, 0) AS Adjusted_Order_Count&#xD;&#xA;    FROM&#xD;&#xA;        Orders_With_Data owd&#xD;&#xA;),&#xD;&#xA;-- Krok 7: Obliczenie skumulowanej Adjusted_Order_Count&#xD;&#xA;Orders_Cumulative AS (&#xD;&#xA;    SELECT&#xD;&#xA;        oa.*,&#xD;&#xA;        SUM(oa.Adjusted_Order_Count) OVER (&#xD;&#xA;            PARTITION BY oa.SKU &#xD;&#xA;            ORDER BY oa.`Expected Receipt Date`, oa.`Document No_` &#xD;&#xA;            ROWS BETWEEN UNBOUNDED PRECEDING AND CURRENT ROW&#xD;&#xA;        ) AS Cumulative_Adjusted_Order_Count&#xD;&#xA;    FROM&#xD;&#xA;        Orders_Adjusted oa&#xD;&#xA;),&#xD;&#xA;-- Krok 8: Alokacja WIP na podstawie Adjusted_Order_Count&#xD;&#xA;Allocation_Result AS (&#xD;&#xA;    SELECT&#xD;&#xA;        o.*,&#xD;&#xA;        CASE&#xD;&#xA;            WHEN o.Cumulative_Adjusted_Order_Count &lt;= o.Total_WIP_Count THEN o.Adjusted_Order_Count&#xD;&#xA;            WHEN o.Cumulative_Adjusted_Order_Count - o.Adjusted_Order_Count &lt; o.Total_WIP_Count THEN o.Total_WIP_Count - (o.Cumulative_Adjusted_Order_Count - o.Adjusted_Order_Count)&#xD;&#xA;            ELSE 0&#xD;&#xA;        END AS Allocated_WIP_Count&#xD;&#xA;    FROM&#xD;&#xA;        Orders_Cumulative o&#xD;&#xA;)&#xD;&#xA;-- Krok 9: Ostateczne wyniki&#xD;&#xA;SELECT&#xD;&#xA;    ar.`Document No_`,&#xD;&#xA;    ar.`Expected Receipt Date`,&#xD;&#xA;    ar.SKU,&#xD;&#xA;    ar.Description,&#xD;&#xA;    ar.order_by,&#xD;&#xA;    ar.Order_Count,&#xD;&#xA;    ar.Packaging_Count,&#xD;&#xA;    ar.Shipping_Count,&#xD;&#xA;    ar.Adjusted_Order_Count,&#xD;&#xA;    ar.Allocated_WIP_Count&#xD;&#xA;FROM&#xD;&#xA;    Allocation_Result ar&#xD;&#xA;ORDER BY&#xD;&#xA;    ar.SKU, ar.`Expected Receipt Date`;&#xD;&#xA;&quot;"/>
